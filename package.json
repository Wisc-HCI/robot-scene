{
  "name": "robot-scene",
  "version": "0.8.2",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "private": false,
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "http://Wisc-HCI.github.io/robot-scene",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wisc-HCI/robot-scene.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.3.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "immer": "^9.0.12",
    "lodash": "^4.17.21",
    "number-precision": "^1.5.2",
    "postprocessing": "^6.26.2",
    "react-scripts": "5.0.1",
    "@react-three/drei": "^9.2.0",
    "@react-three/fiber": "^8.0.10",
    "@react-three/postprocessing": "^2.3.2",
    "web-vitals": "^2.1.4",
    "zustand": "^4.0.0-rc.1"
  },
  "scripts": {
    "start": "npm run storybook",
    "build": "npm run build-storybook",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public --output-dir docs",
    "clean": "rimraf dist",
    "compile": "rollup -c",
    "generate-all": "npm run compile && npm run build-storybook"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three-stdlib": "^2.12.1",
    "three": "^0.142.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "webpack": "^5.73.0",
    "three-stdlib": "^2.12.1",
    "three": "^0.142.0"
  }
